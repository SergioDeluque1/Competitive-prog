#include <iostream>
#include <vector>

int main() {
    // Initialize the first two Fibonacci numbers
    std::vector<long long> fibonacci(100);

    // First two terms
    fibonacci[0] = 0;
    fibonacci[1] = 1;

    // Calculate the next 98 terms
    for (int i = 2; i < 100; i++) {
        fibonacci[i] = fibonacci[i - 1] + fibonacci[i - 2];
    }

    // Print the first 100 Fibonacci numbers
    std::cout << "First 100 Fibonacci numbers are: " << std::endl;
    for (int i = 0; i < 100; i++) {
        std::cout << fibonacci[i] << " ";
    }

    return 0;
}
